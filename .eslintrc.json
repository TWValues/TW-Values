{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prop-types": "off"
  },
  "overrides": [
    {
      "files": ["src/**/*.js", "src/**/*.jsx"]
    }
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
